// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id
  email     String   @unique
  createdAt DateTime @default(now())
  hash      String
  hashedrf  String?
  updatedAt DateTime @updatedAt
}

model login {
  email String  @unique
  name  String?
  age   Int
}
